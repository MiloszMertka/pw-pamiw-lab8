<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" alignment="CENTER" spacing="20.0"
      fx:controller="com.example.client.view.EquipmentOptionsViewModel">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Equipment"/>

    <TableView fx:id="table"/>
    <Pagination fx:id="pagination"/>

    <HBox alignment="CENTER" spacing="20.0">
        <Button text="Go back" onAction="#onGoBackButtonClick"/>
        <Button text="Create new equipment" onAction="#onCreateNewEquipmentOptionButtonClick"/>
    </HBox>

</VBox>
